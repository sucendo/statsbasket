            }
            
            function escribe_json(data) {
                var json = JSON.stringify(data, null, 4);
                var blob = new Blob([json], {type: "application/json"});
                var url  = URL.createObjectURL(blob);
                
                var a = document.createElement('a');
                
                var f = new Date();
                var fecha = (f.getDate() + "/" + (f.getMonth() +1) + "/" + f.getFullYear());   
                a.download    = 'partido'+fecha+'.json';
                a.href        = url;
                a.textContent = 'Descargar';
                
                document.getElementById('content').appendChild(a);
            };		
			
			function lee_clasificacion(tipo,numero) {
				
				if (tipo == null){
					tipo = "semana";
					numero = 22;
				}
            				
				$.getJSON("partidos.json", function(datos) {
            
					//estadísticas temporada
					var table = document.getElementById('clasificacion');
					table.innerHTML = "";
					
					var div = document.getElementById('table-c');
					div.innerHTML = "";  
					div.appendChild(table);
									 
					var thead = document.createElement('thead');             
					
					thead.innerHTML += '<tr><th>Pos</th><th>Equipo</th><th>Pt</th><th>PJ</th><th>PG</th><th>PP</th><th>PF</th><th>PC</th><th>DP</th></tr>';
					  
					table.appendChild(thead); 
						
					var tbody = document.createElement('tbody');
					tbody.id = "clasificacion";
					
					var jsonC = [];
	 
					$.each(datos["equipos"], function(idx,equipo) {
																
						var dat = ['',0,0,0,0,0,0,0];
						
						dat[0] = equipo["nombre"];
						var pnd = 0;
								
						$.each(datos["partidos"], function(idx,partido) {
						
							if (partido[tipo] <= numero){
								
								var pt_local = 0;
								var pt_visitante = 0;
								var eq_local = 0;
								var eq_visitante = 0;
								var pj = 2;
								
								$.each(partido["equipos"], function(idx,equipos) {  							
									if (idx==0){
										pt_local = equipos["puntos"];
										eq_local = equipos["equipo"];
									}else{
										pt_visitante = equipos["puntos"]; 
										eq_visitante = equipos["equipo"]; 
									}
								
									if ((equipos["equipo"] == equipo["numero"])){
										pj = idx;
										dat[2]++;
									}									
								});
								
								if (pj != 2){
									if (pj==0){
										dat[5] += pt_local;
										dat[6] += pt_visitante;
										dat[7] += pt_local - pt_visitante;
										if (pt_local > pt_visitante){
											dat[1]+=2;dat[3]++;
										}else{
											dat[1]++;dat[4]++;
										}
										if ((pt_local == 0) && (pt_visitante == 20)){
											//dat[1]--; //Temporada 2021-2022 NO
											dat[0]=dat[0]+" (*)";
											pnd++;
										}
										if ((pt_local == 20) && (pt_visitante == 0)){		
											dat[0]=dat[0]+" (*)";
											pnd++;
										}
									}else{
										dat[5] += pt_visitante;
										dat[6] += pt_local;
										dat[7] += pt_visitante - pt_local;
										if (pt_visitante > pt_local){
											dat[1]+=2;dat[3]++;
										}else{
											dat[1]++;dat[4]++;
										}
										if ((pt_local == 0) && (pt_visitante == 20)){
											dat[0]=dat[0]+" (*)";
											pnd++;
										}
										if ((pt_local == 20) && (pt_visitante == 0)){
											//dat[1]--; //Temporada 2021-2022 NO
											dat[0]=dat[0]+" (*)";
											pnd++;								
										}
									}							
								}	
								
							}

						});

						/*if (pnd != 0) dat[2]=(dat[2] - pnd) + "/" + dat[2];*/
						if (pnd != 0) dat[2]=dat[2] + " (*)";
	
						jsonC[idx] = dat;
						
					});
					
					/*funcion ordenar*/
					jsonC.sort(function (a, b){
						if (b[1] != a[1]){
							return (b[1] - a[1]);
						}else{
							if ((tipo == 'jornada')&&(numero == 6)){ 
							/*pendiente realizar clasificación enfrentamiento directo */
